{
  "name": "byos-mobile",
  "version": "1.0.0",
  "homepage": "./",
  "scripts": {
    "legacy-ssl": "export NODE_OPTIONS=--openssl-legacy-provider",
    "start": "env REACT_NATIVE_PACKAGER_HOSTNAME=192.168.1.35 expo start",
    "start:dev-client": "expo start --dev-client",
    "start:tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "NODE_OPTIONS=--openssl-legacy-provider expo export:web",
    "github:build": "expo export:web",
    "web:insecure": "NODE_OPTIONS=--openssl-legacy-provider expo start --web",
    "lint": "npx eslint \"src/**\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.186.0",
    "@aws-sdk/types": "^3.188.0",
    "@aws-sdk/util-base64-browser": "^3.188.0",
    "@azure/msal-browser": "^2.29.0",
    "@azure/msal-react": "^1.4.8",
    "@emotion/react": "^11.10.4",
    "@expo/webpack-config": "^0.17.0",
    "@gorhom/portal": "^1.0.14",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@tanstack/react-query": "^4.10.3",
    "@tanstack/react-table": "^8.5.15",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.1.3",
    "dayjs": "^1.11.5",
    "events": "^3.3.0",
    "expo": "~46.0.13",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "pretty-bytes": "^6.0.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^7.36.1",
    "react-native": "0.69.6",
    "react-native-get-random-values": "^1.8.0",
    "react-native-paper": "5.0.0-rc.6",
    "react-native-responsive-dimensions": "^3.1.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "~0.18.7",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "readable-stream": "^4.2.0",
    "readable-stream2-react-native": "^2.3.9",
    "rn-fetch-blob": "^0.12.0",
    "stream": "^0.0.2",
    "typescript": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/ngrok": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": ">=7",
    "lint-staged": ">=10",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix --config ./.eslintrc.json"
  }
}
